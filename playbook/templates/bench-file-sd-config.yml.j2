{% for n in range(fake_targets_count) %}
- targets: ['{{ fake_ips|random(seed=n) }}:8888']
  labels:
    hostname: node-{{ cluster_id| default('default') }}-{{ n }}
    instance: node-{{ cluster_id| default('default') }}-{{ n }}
    host_number: cfg_{{ n }}
    job: node-exporter
    dc: dc{{ fake_dc_count |random(seed=n) }}
{% endfor %}
